@model List<CustomerSite.BusinessLogic.CustomerViewModel>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>Phone</th>
            <th>Credit Limit</th>
            <th>Customer Since</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
                <td>@item.CreditLimit</td>
                <td>@item.CustomerSince</td>
                <td>
                    <input type="button" value="Edit" class="btn-info" data-bind="click: function() { editCustomer(@item.Id) }" />
                    <input type="button" value="Delete" class="btn-danger" data-bind="click: function() { deleteCustomer(@item.Id) }" />
                </td>
            </tr>
        }
    </tbody>
</table>